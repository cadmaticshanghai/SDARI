ladder_bolt_switch_handler(item, event_type, dummy)
{
    res = W_GET_PANELITEM_ARG(item, W_PANEL_VALUE);
	ladder_bolt_switch(res);
}

ladder_bolt_switch(flag)
{
	if(flag=="0"){
        W_SET_PANELITEM_ARGS(Ladder_Part3, W_PANEL_SENSITIVITY,0);
        W_SET_PANELITEM_ARGS(Select_Ladder_Part3, W_PANEL_SENSITIVITY,0);
        Bolt_Switch = "off";
        SET_STRING_DEFAULT("StraightLadder102", "Bolt_Switch", "off");
	}
	else if(flag=="1"){
	    W_SET_PANELITEM_ARGS(Ladder_Part3, W_PANEL_SENSITIVITY,1);
	    W_SET_PANELITEM_ARGS(Select_Ladder_Part3, W_PANEL_SENSITIVITY,1);
	    Bolt_Switch = "on";
	    SET_STRING_DEFAULT("StraightLadder102", "Bolt_Switch", "on");
	}
    if(Modify_flag){
        st = PM_UM_OPEN_CHANGE("修改直梯螺栓");
        calculate_ladder_para();
        delete_ladder_members();
        res = create_ladder(); 
        if (res == -1){
            PM_UM_CLOSE_CHANGE();
            PM_UM_UNDO_LAST_CHANGE();
            return(0);
        }  
        st = PM_UM_CLOSE_CHANGE();
    }
}

