                    return(-1);
                }
            }

            Ladder_Size_bd = value;
            st = SET_FLOAT_DEFAULT("StraightLadder102", "Ladder_Para_bd", value);

            if(Modify_flag){
                /* 定义UNDO缓存 define cache for undo */
                st = PM_UM_OPEN_CHANGE("修改直梯参数:bd");
                calculate_ladder_para();
                delete_ladder_members();
                res = create_ladder(); 
