            rotate_part(px,py,pz, Ladder_X_Dx,Ladder_X_Dy,Ladder_X_Dz, angle, part_handle);
        }        
    } 

    /* 生成零件6-4 create part 6-4*/
    if(Pad_Switch == "on"){
        size_a = Ladder_Size_a; 
        size_b = Ladder_Size_b; 
        size_r = Ladder_Size_r;
        hole_list = alloc_2darray(0, 0);  
        /*calculate_hole_list(hole_list,Ladder_Size_W);*/
        curve = create_rect_curve_with_radius(size_a,size_b,size_r);
        
        lox = A_GET(Part64,0);
        loy = A_GET(Part64,1);
        loz = A_GET(Part64,2);	
        udx = A_GET(Part64,3);
        udy = A_GET(Part64,4);
        udz = A_GET(Part64,5);
        vdx = A_GET(Part64,6);
        vdy = A_GET(Part64,7);
        vdz = A_GET(Part64,8); 

        tmat_h = PM_CREATE_TMAT(1,lox,loy,loz,udx,udy,udz,vdx,vdy,vdz);	    	 
        part_handle = Create_Plate(Ladder_Part6_Pid, Info_System_Id, curve, tmat_h, hole_list);         
        if(ISINT(part_handle)){
            free_2darray(hole_list);
            st = PM_UM_CLOSE_CHANGE();
            return(-1);
        }   
        
        Set_Attribute_Value(part_handle, PartNumber,  "6-4");	
        PM_ADD_OBJECT_TO_SET(part_handle, ladder); 		  
        free_2darray(hole_list); 
        
        /* 旋转垫板 */
        angle = Ladder_Size_bd-90;
        if(Abs(angle)>0.01){
            px = lox; py = loy; pz = loz;
            Point_3D_Move_With_DirAndDis(px,py,pz,Ladder_Y_Dx,Ladder_Y_Dy,Ladder_Y_Dz,-Ladder_Size_d2);
            rotate_part(px,py,pz, Ladder_X_Dx,Ladder_X_Dy,Ladder_X_Dz, angle, part_handle);
        }        
    } 
    
    PM_ADD_SET_TO_GROUP(ladder, Ladder_Handle);
    set_ladder_para(Ladder_Handle);
    save_model_size();
    PM_FREE_SET(ladder);   
	return(0);
}

calculate_hole_list(hole_list,hole_dia)
{
	hole1_size = "circle1 = CURVE(" + STRINGTERM(FTOASCII(hole_dia),".") + "\/ 2,0,ARC,0,0,360)\;";
	hole1_pos  = ",circle1,0,0";		
	
	/* 将开孔信息存入数组 */
	put_2darray(hole_list,0,0,hole1_size);
	put_2darray(hole_list,0,1,hole1_pos);	
	return(0);  
}

