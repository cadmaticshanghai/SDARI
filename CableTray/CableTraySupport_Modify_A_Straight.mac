calculate_support_direction()
{
    tray = Cable_Tray_Handle;
    /*get connection point coordinate*/
    node = 1;
    x1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"x"));
    y1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"y"));
    z1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"z"));
    node = 2;
    x2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"x"));
    y2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"y"));
    z2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"z"));
    
    /*set cursor position*/
    x = (x1+x2)/2;
    y = (y1+y2)/2;
    z = (z1+z2)/2;
    PM_SET_CURRENT_LOC(x,y,z);
    
    /*calculate support direction*/
    dx = x2 - x1;
    dy = y2 - y1;
    dz = z2 - z1;
    length = VEC_UNITV(dx,dy,dz);
    Tray_Length = length;
    Support_U_Dx = dx;
    Support_U_Dy = dy;
    Support_U_Dz = dz;

    node = 2;
    wxr = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"wxr"));
    wxs = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"wxs"));
    Support_V_Dx = COS(wxs)*COS(wxr);
    Support_V_Dy = COS(wxs)*SIN(wxr);
    Support_V_Dz = SIN(wxs);
    
    if(!Support_Direction){
        Support_V_Dx = Support_V_Dx * -1;
        Support_V_Dy = Support_V_Dy * -1;
        Support_V_Dz = Support_V_Dz * -1;
    }
    
    dx1 = Support_V_Dx;
    dy1 = Support_V_Dy;
    dz1 = Support_V_Dz;   
    VEC_CROSS_PRODUCT(dx,dy,dz,dx1,dy1,dz1,Support_W_Dx,Support_W_Dy,Support_W_Dz);
    return(0);
}

calculate_tray_data()
{  
    tray = Cable_Tray_Handle;
    /*get connection point coordinate*/
    node = 1;
    x1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"x"));
    y1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"y"));
    z1 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"z"));
    node = 2;
    x2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"x"));
    y2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"y"));
    z2 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"z"));
       
    part_id = PM_GET_OBJDATA(tray,0,MMT_TAG_PARTID);
    h = 1.0 * DM_PARTID_NAMED_DIM(part_id,"Height");
    w = 1.0 * DM_PARTID_NAMED_DIM(part_id,"Width");
    
    side_bar_width = DM_PARTID_NAMED_DIM(part_id,"SideBarW");
    if(side_bar_width != -1){
        w = w + 2*side_bar_width;
    }
    
    Tray_Width  = w;
    Tray_Height = h;
    st = 0;
    L0 = W_GET_NUMERIC_VALUE(Support_Interval0, st);
    calculate_support_direction();
    /* 如果托架敷线面朝向支撑里面 */
    if(Support_Direction){
        if(Align_Position == "B"){
            Support_X = x1;
            Support_Y = y1;
            Support_Z = z1;
        }
        else if(Align_Position == "T"){
            Support_X = x1 + Support_W_Dx*h;
            Support_Y = y1 + Support_W_Dy*h;
            Support_Z = z1 + Support_W_Dz*h;
        }
        else if(Align_Position == "M"){
            Support_X = x1 + Support_W_Dx*h/2;
            Support_Y = y1 + Support_W_Dy*h/2;
            Support_Z = z1 + Support_W_Dz*h/2;
        }
    }
    /* 如果托架敷线面背离支撑面 */
    else{
        if(Align_Position == "B"){
            Support_X = x1 - Support_W_Dx*h;
            Support_Y = y1 - Support_W_Dy*h;
            Support_Z = z1 - Support_W_Dz*h;
        }
        else if(Align_Position == "T"){
            Support_X = x1;
            Support_Y = y1;
            Support_Z = z1;
        }
        else if(Align_Position == "M"){
            Support_X = x1 - Support_W_Dx*h/2;
            Support_Y = y1 - Support_W_Dy*h/2;
            Support_Z = z1 - Support_W_Dz*h/2;
        }   
    }
    return(0);
}

calculate_leg_number()
{
    st = 0;
    L0 = W_GET_NUMERIC_VALUE(Support_Interval0, st);
    leg_size = 1.0*DM_PARTID_DATA(Support_Leg_Pid,"D31");
    length = Tray_Length;
    interval = Max_Interval;
    n_f = (length - 2*L0-leg_size) / interval;
    n = String_To_Int(STRINGTERM(FTOASCII(n_f),"."));
    remainder = length - n*interval;
    if(remainder == 0){
        Leg_Number = n+1;
    }
    else{
        Leg_Number = n+2;
    }
    return(0);
}

calculate_leg_interval()
{
    st = 0;
    L0 = W_GET_NUMERIC_VALUE(Support_Interval0, st);
    /* leg_size = 1.0*DM_PARTID_DATA(Support_Leg_Pid,"D31"); */
    length = Tray_Length;
    Leg_Interval = 1.0*(length - 2*L0)/(Leg_Number - 1);
    
    temp = "";
    S_PRINTF(temp, "%.0f", Leg_Interval);
    Leg_Interval = String_To_Float(temp);
    /* U_MESSAGE("Leg_Interval="+FTOASCII(Leg_Interval)); */
    
    A_PUT(Intervals,0,L0);
    for(i=1;i<Leg_Number;i=i+1){
        A_PUT(Intervals,i,Leg_Interval);
    }
    return(0);
}

