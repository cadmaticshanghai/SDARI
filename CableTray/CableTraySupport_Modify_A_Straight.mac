        x2=get_2darray(Legs,2*i,3);
        y2=get_2darray(Legs,2*i,4);
        z2=get_2darray(Legs,2*i,5);
        put_2darray(Pads,2*i,0,x2);
        put_2darray(Pads,2*i,1,y2);
        put_2darray(Pads,2*i,2,z2);
        x2=get_2darray(Legs,2*i+1,3);
        y2=get_2darray(Legs,2*i+1,4);
        z2=get_2darray(Legs,2*i+1,5);
        put_2darray(Pads,2*i+1,0,x2);
        put_2darray(Pads,2*i+1,1,y2);
        put_2darray(Pads,2*i+1,2,z2);
    }

    /*calculate L0 and aling position*/
    x1 = get_2darray(Legs,0,0);
    y1 = get_2darray(Legs,0,1);
    z1 = get_2darray(Legs,0,2);
    x2 = get_2darray(Legs,1,0);
    y2 = get_2darray(Legs,1,1);
    z2 = get_2darray(Legs,1,2);
   
    tray = Cable_Tray_Handle;
    leg_size = 1.0*DM_PARTID_DATA(Support_Leg_Pid,"D31");
    
    /* cable tray layer*/
    layer = get_tray_layer();
    Support_Layer = layer;
    W_SET_PANELITEM_ARGS(Support_Layer_Option, W_PANEL_ORDINALVALUE, layer-1);
    set_support_layer_option(layer);
    
    /*get connection point coordinate*/
    node = 1;
    x3 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"x"));
    y3 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"y"));
    z3 = String_To_Float(PM_GET_CONPNT_DATA(tray,node,"z"));
    x4 = 0;y4 = 0;z4 = 0;
    point_set = PM_CREATE_PNTSET(4);
    PM_SET_PNT(point_set,0,x1,y1,z1);
    PM_SET_PNT(point_set,1,x2,y2,z2);
    PM_SET_PNT(point_set,2,x3,y3,z3);
    PM_SET_PNT(point_set,3,x4,y4,z4);
    distance = 0;
    res = Project_Point_To_Line(point_set,distance);
    PM_GET_PNT(point_set,3,x4,y4,z4);
    PM_FREE_PNTSET(point_set);
    dx = Support_U_Dx;dy=Support_U_Dy;dz=Support_U_Dz;
    x5=0;y5=0;z5=0;
    PlaneLineIntersectPoint(dx,dy,dz,x1,y1,z1,dx,dy,dz,x3,y3,z3,x5,y5,z5);  
    dx=x5-x4;dy=y5-y4;dz=z5-z4;
    distance = VEC_UNITV(dx,dy,dz);
    Support_X = x3;Support_Y= y3;Support_Z = z3;
    Point_3D_Move_With_DirAndDis(Support_X,Support_Y,Support_Z,Support_W_Dx,Support_W_Dy,Support_W_Dz,-distance);
    if(Support_Direction){
        if(distance < 1 & distance > -1){
            Align_Position = "B";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 0);
        }
        check = distance - Tray_Height/2;
        if(check < 1 & check > -1){
            Align_Position = "M";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 1);
        }   
        check = distance - Tray_Height;
        if(check < 1 & check > -1){
            Align_Position = "T";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 2);
        }   
    }
    else{
        check = distance - Tray_Height;
        if(check < 1 & check > -1){
            Align_Position = "B";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 0);
        }  
        check = distance - Tray_Height/2;
        if(check < 1 & check > -1){
            Align_Position = "M";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 1);
        }  
        if(distance < 1 & distance > -1){
            Align_Position = "T";
            W_SET_PANELITEM_ARGS(Support_Align_Option, W_PANEL_ORDINALVALUE, 2);
        }
    }

    dx=x5-x3;dy=y5-y3;dz=z5-z3;
    interval0 = VEC_UNITV(dx,dy,dz)-0.5*leg_size;
    A_PUT(Intervals,0,interval0);
    W_SET_NUMERIC_VALUE(Support_Interval0,interval0);   
    return(0);
}

