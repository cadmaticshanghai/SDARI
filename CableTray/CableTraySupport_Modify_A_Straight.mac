    for(i=1;i<1000;i=i+1;){
        support_name = pre + "-" + HEAD(index_number,STRLEN(index_number) - STRLEN(ITOASCII(i))) + ITOASCII(i);
        find = PM_FIND_BY_NAME(support_name);
        if(ISINT(find)){
